@model RQLib.RQQueryResult.RQDescriptionElements.RQDescriptionElement
@if (ViewData["label"] != null)
{
    @Html.LabelFor(model => model, ViewData["label"].ToString())
}
else
{ 
    @Html.LabelFor(model => model)
}
@Html.TextBoxFor(model => model, new { @class = ViewData["classname"] })
@Html.ValidationMessageFor(model => model)