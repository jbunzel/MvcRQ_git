    <rdf:type rdf:resource="http://dbpedia.org/class/yago/TranscriptionFactors"/>
    <owl:sameAs rdf:resource="http://dbpedia.org/resource/Hepatocyte_nuclear_factor_4"/>
    <foaf:page rdf:resource="http://en.wikipedia.org/wiki/Hepatocyte_nuclear_factor_4"/>
